{
  "name": "ec-education-web",
  "version": "0.1.0",
  "description": "ES6 with Babel and Webpack",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf node_modules && npm install",
    "dev-ec": "theme=ec node webpack.dev.js",
    "dev-smh": "theme=smh node webpack.dev.js",
    "stage-ec": "theme=ec webpack --config webpack.stage.js --progress --colors -p",
    "stage-smh": "theme=smh webpack --config webpack.stage.js --progress --colors -p",
    "build-ec": "theme=ec webpack --config webpack.prod.js --progress --colors -p",
    "build-smh": "theme=smh webpack --config webpack.prod.js --progress --colors -p",
    "test": "eslint sourcecode"
  },
